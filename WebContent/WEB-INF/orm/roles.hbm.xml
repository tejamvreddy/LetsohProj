<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.letsoh.model.LetsohRoles" table="TB_ROLES" catalog="LETSOH_DEV">
        <id name="roleName" type="string">
            <column name="ROLE_NAME" length="45" not-null="true"/>
        </id>
        <property name="roleDesc" type="string">
            <column name="ROLE_DESC" length="45" not-null="true" />
        </property>
        
    </class>
</hibernate-mapping>